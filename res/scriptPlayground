--你可以在这里测试KiKoPlay的各种脚本API
print("------KikoPlay Script Playground------")
local envinfo = kiko.envinfo()
for k, v in pairs(envinfo) do
    print(string.format("%s:\t%s", k, v))
end
--查看KikoPlay API，具体用法参考目录下的KikoPlay脚本开发参考.pdf
--或者 https://github.com/KikoPlayProject/KikoPlayScript
kiko.viewtable(kiko)
