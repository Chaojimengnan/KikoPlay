/* ScrollBar Setting */
QWidget[cScrollStyle="true"] QScrollBar:horizontal{
    background: transparent;
    height: 10px;
}
QWidget[cScrollStyle="true"] QScrollBar::handle:horizontal{
    background: rgba(255,255,255,180);
    min-height: 70px;
    border-radius: 1px;
}
QWidget[cScrollStyle="true"] QScrollBar::handle:horizontal:hover{
    background: rgba(255,255,255,255);
}
QWidget[cScrollStyle="true"] QScrollBar::handle:horizontal:pressed{
    background: rgba(255,255,255,200);
}
QWidget[cScrollStyle="true"] QScrollBar::add-line:horizontal{
    background: none;
}
QWidget[cScrollStyle="true"] QScrollBar::sub-line:horizontal{
    background: none;
}
QWidget[cScrollStyle="true"] QScrollBar::add-page:horizontal,
QWidget[cScrollStyle="true"] QScrollBar::sub-page:horizontal{
    background: none;
}


QWidget[cScrollStyle="true"] QScrollBar:vertical{
    background: transparent;
    width: 10px;
}
QWidget[cScrollStyle="true"] QScrollBar::handle:vertical{
    background: rgba(255,255,255,180);
    min-height: 70px;
    border-radius: 1px;
}
QWidget[cScrollStyle="true"] QScrollBar::handle:vertical:hover {
    background: rgba(255,255,255,255);
}
QWidget[cScrollStyle="true"] QScrollBar::handle:vertical:pressed {
    background: rgba(255,255,255,200);
}
QWidget[cScrollStyle="true"] QScrollBar::add-line:vertical{
    background: none;
}
QWidget[cScrollStyle="true"] QScrollBar::sub-line:vertical{
    background: none;
}
QWidget[cScrollStyle="true"] QScrollBar::add-page:vertical,
QWidget[cScrollStyle="true"] QScrollBar::sub-page:vertical{
    background: none;
}
/* --ScrollBar Setting-- */


